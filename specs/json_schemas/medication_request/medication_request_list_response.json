{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Medication Request identifier"
          },
          "status": {
            "type": "string",
             "description": "Medication Request status. Find available **statuses** on State Charts wiki page"
          },
          "request_number": {
            "type": "string",
            "description": "Public medication request human readable number"
          },
          "created_at": {
            "type": "string",
            "description": "Medication request creation date, which is determined by the external system. Format *DATE '2017-09-07'*"
          },
          "started_at": {
            "type": "string",
            "description": "Start date of a treatment period, which is determined by the external system. Greater or equal to **created_at**. Format *DATE '2017-09-07'*"
          },
          "ended_at": {
            "type": "string",
            "description": "End date of a treatment period, which is determined by the external system. Greater or equal to **started_at**. Format *DATE '2017-10-07'*"
          },
          "dispense_valid_from": {
            "type": "string",
            "description": "Start date of dispense period, which is determined by the external system. Format *DATE '2017-09-07'*"
          },
          "dispense_valid_to": {
            "type": "string",
            "description": "End date of dispense period, which is determined by the external system. Greater or equal to **dispense_valid_from**. Format *DATE '2017-10-07'*"
          },
          "legal_entity": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "short_name": {
                "type": "string"
              },
              "public_name": {
                "type": "string"
              },
              "type": {
                "type": "string"
              },
              "edrpou": {
                "type": "string"
              },
              "status": {
                "type": "string",
                "enum": [
                  "ACTIVE",
                  "CLOSED"
                ]
              }
            },
            "required": [
              "id",
              "name",
              "short_name",
              "public_name",
              "type",
              "edrpou",
              "status"
            ]
          },
          "division": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "addresses": {
                "type": "array"
              },
              "phones": {
                "type": "array"
              },
              "email": {
                "type": "string"
              },
              "type": {
                "type": "string",
                "enum": [
                  "CLINIC",
                  "AMBULANT_CLINIC",
                  "FAP"
                ]
              },
              "external_id": {
                "type": "string"
              },
              "location": {
                "type": ["object", "null"],
                "properties": {
                  "latitude": {
                    "type": "number"
                  },
                  "longitude": {
                    "type": "number"
                  }
                },
                "required": [
                  "latitude",
                  "longitude"
                ]
              }
            },
            "required": [
              "id",
              "name",
              "addresses",
              "phones",
              "email",
              "type"
            ]
          },
          "employee": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "position": {
                "type": "string",
                "description": "`Dictionary POSITION`"
              },
              "party": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string"
                  },
                  "first_name": {
                    "type": "string"
                  },
                  "last_name": {
                    "type": "string"
                  },
                  "second_name": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "phones": {
                    "type": "array"
                  },
                  "tax_id": {
                    "not": {}
                  },
                  "documents": {
                    "not": {}
                  }
                },
                "required": [
                  "id",
                  "first_name",
                  "last_name"
                ]
              }
            },
            "required": [
              "id",
              "position"
            ]
          },
          "person": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "first_name": {
                "type": "string"
              },
              "last_name": {
                "type": "string"
              },
              "second_name": {
                "type": "string"
              },
              "birth_date": {
                "type": "string",
                "description": "ISO Datetime."
              },
              "addresses": {
                "type": "array"
              }
            },
            "required": [
              "id",
              "first_name",
              "last_name",
              "birth_date",
              "addresses"
            ]
          },
          "medication_info": {
            "type": "object",
            "properties": {
              "medication_id": {
                "type": "string",
                "description": "`Identifier medication with type **INNM_DOSAGE**`"
              },
              "form": {
                "type": "string",
                "description": "`Dictionary MEDICATION_FORM`"
              },
              "dosage": {
                "type": "object",
                "properties": {
                  "numerator_unit": {
                    "type": "string",
                    "description": "`Dictionary MEDICATION_UNIT`"
                  },
                  "numerator_value": {
                    "type": "number"
                  },
                  "denumerator_unit": {
                    "type": "string",
                    "description": "`Dictionary MEDICATION_UNIT`"
                  },
                  "denumerator_value": {
                    "type": "number"
                  }
                },
                "required": [
                  "numerator_unit",
                  "numerator_value",
                  "denumerator_unit",
                  "denumerator_value"
                ],
                "description": "Dosage of innm_dosage object"
              },
              "medication_qty": {
                "type": "number",
                "description": "Medication quantity isuued by the doctor"
              },
              "medication_name": {
                "type": "string"
              }
            },
            "required": [
              "medication_id",
              "form",
              "dosage",
              "medication_qty",
              "medication_name"
            ]
          },
          "medical_program": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "`Medical program Identifier`"
              },
              "name": {
                "type": "string",
                "description": "`Medical program name`"
              }
            },
            "required": [
              "id",
              "name"
            ]
          }
        },
        "required": [
          "id",
          "status",
          "created_at",
          "started_at",
          "ended_at",
          "dispense_valid_from",
          "dispense_valid_to",
          "legal_entity",
          "division",
          "employee",
          "medication_info",
          "medical_program"
        ]
      }
    },
    "paging": {
      "type": "object",
      "properties": {
        "page_number": {
          "type": "number",
          "description": "Page number."
        },
        "page_size": {
          "type": "number",
          "description": "A limit on the number of objects to be returned, between 1 and 100. Default: 50"
        },
        "total_entries": {
          "type": "number",
          "description": "Total number of objects in collection."
        },
        "total_pages": {
          "type": "number",
          "description": "Total number of pages."
        }
      },
      "required": [
        "page_number",
        "page_size",
        "total_entries",
        "total_pages"
      ],
      "additionalProperties": false
    }
  }
}
